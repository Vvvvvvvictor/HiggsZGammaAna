{
    "name": "zgamma_preselection",
    "function": {
        "module_name": "higgs_dna.analysis",
        "function_name": "run_analysis"
    },
    "tag_sequence": [
        {
            "module_name": "higgs_dna.taggers.zgamma_tagger_run2",
            "tagger": "ZGammaTaggerRun2",
            "kwargs": {
                "name": "zgamma_tagger_run2",
                "options": {
                    "gen_info": {
                        "calculate": true
                    }
                }
            }
        }
    ],
    "systematics": {
        "weights": {
            "electron_veto_sf": {
                "type": "object",
                "method": "from_function",
                "function": {
                    "module_name": "higgs_dna.systematics.photon_systematics",
                    "name": "photon_electron_veto_sf"
                },
                "modify_central_weight": true,
                "input_collection": "Photon",
                "target_collections": [
                    [
                        "Photon"
                    ]
                ]
            },
            "pu_reweight_sf": {
                "type": "event",
                "method": "from_function",
                "function": {
                    "module_name": "higgs_dna.systematics.pileup_systematics",
                    "name": "pu_reweight_sf"
                },
                "modify_central_weight": true,
                "requires_branches": [
                    "Pileup_nTrueInt"
                ]
            },
            "btag_deepjet_wp_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.jet_systematics",
                    "name" : "btag_deepjet_wp_sf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Jet",
                "target_collections" : ["SelectedJet"],
                "working_point" : "M"
            },
            "electron_wplid_sf": {
                "type": "object",
                "method": "from_function",
                "function": {
                    "module_name": "higgs_dna.systematics.lepton_systematics",
                    "name": "electron_WPL_sf"
                },
                "modify_central_weight": true,
                "input_collection": "Electron",
                "target_collections": [
                    "SelectedElectron"
                ]
            },
            "muon_looseid_sf": {
                "type": "object",
                "method": "from_function",
                "function": {
                    "module_name": "higgs_dna.systematics.lepton_systematics",
                    "name": "muon_LooseID_sf"
                },
                "modify_central_weight": true,
                "input_collection": "Muon",
                "target_collections": [
                    "SelectedMuon"
                ]
            }
        },
        "independent_collections": {
            "FNUF": {
                "method": "from_function",
                "branch_modified": [
                    "Photon",
                    "pt"
                ],
                "function": {
                    "module_name": "higgs_dna.systematics.photon_systematics",
                    "name": "fnuf_unc"
                }
            },
            "Material": {
                "method": "from_function",
                "branch_modified": [
                    "Photon",
                    "pt"
                ],
                "function": {
                    "module_name": "higgs_dna.systematics.photon_systematics",
                    "name": "material_unc"
                }
            },
            "JER": {
                "method": "from_branch",
                "branch_modified": [
                    "Jet",
                    "pt"
                ],
                "branches": {
                    "up": [
                        "Jet",
                        "pt_jerUp"
                    ],
                    "down": [
                        "Jet",
                        "pt_jerDown"
                    ]
                }
            },
            "JES": {
                "method": "from_branch",
                "branch_modified": [
                    "Jet",
                    "pt"
                ],
                "branches": {
                    "up": [
                        "Jet",
                        "pt_jesTotalUp"
                    ],
                    "down": [
                        "Jet",
                        "pt_jesTotalDown"
                    ]
                }
            },
            "MET_JES": {
                "method": "from_branch",
                "branch_modified": "MET_pt",
                "branches": {
                    "up": "MET_T1_pt_jesTotalUp",
                    "down": "MET_T1_pt_jesTotalDown"
                }
            },
            "MET_Unclustered": {
                "method": "from_branch",
                "branch_modified": "MET_pt",
                "branches": {
                    "up": "MET_T1_pt_unclustEnUp",
                    "down": "MET_T1_pt_unclustEnDown"
                }
            },
            "Muon_pt": {
                "method": "from_branch",
                "branch_modified": [
                    "Muon",
                    "pt"
                ],
                "branches": {
                    "up": [
                        "Muon",
                        "correctedUp_pt"
                    ],
                    "down": [
                        "Muon",
                        "correctedDown_pt"
                    ]
                }
            }
        }
    },
    "samples": {
        "catalog": "metadata/samples/zgamma_tutorial.json",
        "sample_list": [
            "DYGto2LG_10to50", "DYGto2LG_50to100"
